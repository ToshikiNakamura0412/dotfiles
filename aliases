# Set ROS alias command
alias cw='cd ~/catkin_ws'
alias cs='cd ~/catkin_ws/src'
alias cb='catkin build'
alias cbt='catkin build --this'

# Set Git alias command
alias gs='git status'
alias ga='git add'
alias gc='git commit'
alias gcm='git commit -m'
alias gps='git push'
alias gpl='git pull'

# Set Neovim alias command
alias vi='nvim'
alias vim='nvim'

# Set Neovim alias command
alias xm='xmodmap ~/.Xmodmap'
